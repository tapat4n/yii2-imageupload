{
    "name": "tapat4n/yii2-imageupload",
    "version":"0.3.0",
    "description": "Image upload widget with crop for Yii2",
    "keywords": ["yii2", "image", "upload", "crop"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [],
    "require": {
        "yiisoft/yii2": ">=2.0.4",
        "yiisoft/yii2-bootstrap": "*",
        "bower-asset/cropper":"*",
        "bower-asset/dropzone":"*",
        "fortawesome/font-awesome": "*",
        "imagine/imagine": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "tapat4n\\imageupload\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-branch": "0.0.3-dev"
        }
    }
}
